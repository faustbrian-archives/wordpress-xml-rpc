{
    "name": "faustbrian/wordpress-xml-rpc-php-client",
    "description": "A WordPress XML-RPC Client for PHP",
    "keywords": ["api", "http", "wordpress xml-rpc"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.0",
        "faustbrian/http": "^1.0@dev"
    },
    "require-dev": {
        "coderabbi/phpumoji": "^0.7.0@beta",
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\WordPressXmlRpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\WordPressXmlRpc\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
